.container-fluid
	.row.jumbotron
		.col-xs-12.col-sm-12.col-md-9.col-lg-9.col-xl-10
			p.lead
				| A web
		.col-xs-12.col-sm-12.col-md-3.col-lg-3.col-xl-2
			a(href='#')
				button.btn.btn-outline-secondary.btn-lg(type='button') Web Hosting

#slides.carousel.slide(data-ride='carousel')
	ol.carousel-indicators
		li.active(data-target='#slides'	data-slide-to='0')
		li(data-target='#slides' data-slide-to='1')
		li(data-target='#slides' data-slide-to='2')
		li(data-target='#slides' data-slide-to='3')
	.carousel-inner
		.carousel-item.active
			img(src='images/p1.jpg' alt='First Picture')
		.carousel-item
			img.d-block.w-100(src='images/p2.jpg' alt='Second Picture')
		.carousel-item
			img.d-block.w-100(src='images/p3.jpg' alt='Third Picture')
		.carousel-item
			img.d-block.w-100(src='images/p4.jpg' alt='Four Picture')
